NH PROPS và ko dùng mapStateToProps\n\n    const mangPhim = useSelector(state => state.QuanLyPhimReducer.mangPhim);\n    const dispatch = useDispatch();\n    console.log('MangPhim', mangPhim);\n    const loadDataPhim = async () => {\n        dispatch(getDataFilmAction())\n    }\n    useEffect(() => {\n\n        //Chạy 1 lần duy nhất sau khi giao diện render\n        dispatch(getDataFilmAction())\n        //Vừa vào trang sẽ load danh sách phim\n\n    }, [])\n    \n    // console.log('Props Mang Phim', props.mangPhim);\n\n    // const loadDataPhim = async() => {\n\n    //     // props.dispatch({\n    //     //     type: 'GET_DATA_FILM',\n    //     //     dataFilm: result.data\n    //     // })\n\n    //     props.dispatch(getDataFilmAction())\n    //     // Maintain code dễ hơn, chỉ handle action chi tiết được gọi trong folder actions\n    // }\n\n\n    //useEffect thay thế cho các lifecycle (didMount, didUpdate, didUnmount)\n    \n\n    const renderPhim = () => {\n        return mangPhim.map((phim, index) => {\n            return <div className=\"col-4 mb-2\">\n                <div className=\"card text-left\">\n                    <img className=\"card-img-top\" src={phim.hinhAnh} alt />\n                    <div className=\"card-body\">\n                        <h4 className=\"card-title\">{phim.tenPhim}</h4>\n                        <p className=\"card-text\">{phim.moTa}</p>\n                        <NavLink className=\"btn btn-warning\" to={`/chitietphim/${phim.maPhim}`}>Mua vé</NavLink>\n                    </div>\n                </div>\n\n            </div>\n        })\n    }\n\n    return (\n        <div className=\"container\">\n            <button onClick={() => {\n                loadDataPhim();\n            }}>Load Danh Sách Phim</button>\n            <div className=\"row mt-2\">\n                {renderPhim()}\n            </div>\n        </div>\n    )\n}\n\n\n// //Kết nối với Reducer để lấy dữ liệu mảng phim về => tạo ra được props.mangPhim\n// const mapStateToProps = (state) => {\n//     //trả về 1 state\n\n//     return {\n//         mangPhim: state.QuanLyPhimReducer.mangPhim\n//     }\n// }\n\n// export default connect(mapStateToProps) (TrangChu)","/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/pages/DangNhap/DangNhap.js",[],"/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/pages/DangKy/DangKy.js",[],"/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/pages/ChiTietPhim/ChiTietPhim.js",["83","84"],"import Axios from 'axios';\nimport React, { useEffect, useState } from 'react'\nimport moment from 'moment'\n// import styles from './chiTietPhim.module.scss'\n\nexport default function ChiTietPhim(props) {\n    //Props: lấy dữ liệu ra. props.match.params: props này là props của thẻ Route truyền cho component\n\n    console.log('PropsChiTietPhim', props);\n\n    //Tạo 1 state chứa thông tin chi tiết phim giá trị ban đầu là object rỗng\n    const [chiTietPhim, setChiTietPhim] = useState({});\n\n    // Dùng useEffect để tự động gọi api chi tiết phim khi trang load ra\n    useEffect(async () => {\n\n        const maPhim = props.match.params.maPhim;\n\n        //lấy đc maPhim : gửi lên api backend để lấy dữ liệu phim về và gán vào state chi tiết phim\n\n        const result = await Axios({\n            url: `https://movie0706.cybersoft.edu.vn/api/QuanLyRap/LayThongTinLichChieuPhim?MaPhim=${maPhim}`,\n            method: 'GET'\n        });\n        console.log(result.data);\n        setChiTietPhim(result.data);\n\n        //set title\n        document.title = result.data.tenPhim;\n\n\n    }, []) //tham số thứ 2 là mảng rỗng, chỉ muốn nó chay 1 lần load ra trang này\n\n\n\n    return (\n        <div className=\"container\">\n            <div className=\"row  mt-4\">\n                <div className=\"col-4\">\n                    <img src={chiTietPhim.hinhAnh} alt={chiTietPhim.hinhAnh} style={{ width: '200px', height: '300px' }} />\n                </div>\n                <div className=\"col-8 text-white rounded\" style={{ backgroundColor: '#f7f7f9' }}>\n                    <table className=\"table\">\n                        <tbody>\n                            <tr>\n                                <td>Tên phim</td>\n                                <td>{chiTietPhim.tenPhim}</td>\n                            </tr>\n                            <tr>\n                                <td>Mô tả</td>\n                                <td>{chiTietPhim.moTa}</td>\n                            </tr>\n                        </tbody>\n                    </table>\n                </div>\n\n            </div>\n            <hr className=\"border-light mt-4\" />\n            <div className=\"row mt-5\">\n                <div className=\"col-12 text-center text-white\">\n                    <h3>Thông tin lịch chiếu</h3>\n                </div>\n            </div>\n            <div className=\"row text-white\">\n                <div className=\"nav flex-column nav-pills col-4\" id=\"v-pills-tab\" role=\"tablist\" aria-orientation=\"vertical\">\n                    {chiTietPhim.heThongRapChieu?.map((heThongRap, index) => {\n\n                        const activeClass = index === 0 ? 'active' : '';\n                        \n                        return <a className={`nav-link ${activeClass}`} id={`v-pills-${heThongRap.maHeThongRap}-tab`} data-toggle=\"pill\" href={`#v-pills-${heThongRap.maHeThongRap}`} role=\"tab\" aria-controls={`v-pills-${heThongRap.maHeThongRap}`} aria-selected=\"true\" key={index}>\n                            <img className=\"mr-2\" src={heThongRap.logo} width=\"50\" height=\"50\" alt={heThongRap.logo}/>\n                            {heThongRap.tenHeThongRap}</a>\n                    })}\n                    {/* <a className=\"nav-link active\" id=\"v-pills-home-tab\" data-toggle=\"pill\" href=\"#v-pills-home\" role=\"tab\" aria-controls=\"v-pills-home\" aria-selected=\"true\">Home</a>\n                        <a className=\"nav-link\" id=\"v-pills-profile-tab\" data-toggle=\"pill\" href=\"#v-pills-profile\" role=\"tab\" aria-controls=\"v-pills-profile\" aria-selected=\"false\">Profile</a>\n                        <a className=\"nav-link\" id=\"v-pills-messages-tab\" data-toggle=\"pill\" href=\"#v-pills-messages\" role=\"tab\" aria-controls=\"v-pills-messages\" aria-selected=\"false\">Messages</a>\n                        <a className=\"nav-link\" id=\"v-pills-settings-tab\" data-toggle=\"pill\" href=\"#v-pills-settings\" role=\"tab\" aria-controls=\"v-pills-settings\" aria-selected=\"false\">Settings</a> */}\n                </div>\n                <div className=\"tab-content col-8 text-white\" id=\"v-pills-tabContent\">\n                    {chiTietPhim.heThongRapChieu?.map((heThongRap, index) => {\n                        const activeClass = index === 0 ? 'active' : '';\n\n                        return <div key={index} className={`tab-pane ${activeClass}`} id={`v-pills-${heThongRap.maHeThongRap}`} role=\"tabpanel\" aria-labelledby={`v-pills-${heThongRap.maHeThongRap}`}>\n                            {/* {heThongRap.tenHeThongRap} */}\n                            {heThongRap.cumRapChieu?.map((cumRap,index) =>{\n                                return <div key={index} className=\"mt-3\">\n                                    <h3 className=\"text-warning\">{cumRap.tenCumRap}</h3> \n                                    <hr style={{border:'solid 0.1px #e1e1e1d0'}}/>\n                                    <div className=\"row\">\n                                        {cumRap.lichChieuPhim?.slice(0,12).map((lichChieu,index) => {\n                                            return <div key={index} className=\"col-3\">\n                                                {moment(lichChieu.ngayChieuGioChieu).format('hh:mmA')}\n                                            </div>\n                                        })}\n                                    </div>\n                                </div>\n                            })}\n                        </div>\n                    })}\n                    {/* <div className=\"tab-pane fade show active\" id=\"v-pills-home\" role=\"tabpanel\" aria-labelledby=\"v-pills-home-tab\">Home</div>\n                    <div className=\"tab-pane fade\" id=\"v-pills-profile\" role=\"tabpanel\" aria-labelledby=\"v-pills-profile-tab\">Profile</div>\n                    <div className=\"tab-pane fade\" id=\"v-pills-messages\" role=\"tabpanel\" aria-labelledby=\"v-pills-messages-tab\">Message</div>\n                    <div className=\"tab-pane fade\" id=\"v-pills-settings\" role=\"tabpanel\" aria-labelledby=\"v-pills-settings-tab\">Setting</div> */}\n                </div>\n            </div>\n        </div>\n    )\n}\n","/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/components/Header/Header.js",["85","86","87","88","89"],"/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/redux/reducers/QuanLyPhimReducer.js",["90"],"// mảng phim sẽ chứa dữ liệu phim từ api trả về\n\nimport { GET_DATA_FILM } from \"../const/QuanLyPhimConst\"\n\nconst stateDefault = {\n    mangPhim : []\n\n}\n\nexport const QuanLyPhimReducer = (state = stateDefault, action) => {\n\n    switch(action.type) {\n\n        case GET_DATA_FILM : {\n           \n            return {...state, mangPhim : action.dataFilm}\n        }\n            \n\n    }\n\n\n    return {...state}\n}","/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/redux/actions/QuanLyPhimActions.js",[],"/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/redux/const/QuanLyPhimConst.js",[],"/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/util/settings.js",[],"/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/redux/actions/QuanLyNguoiDungActions.js",[],"/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/templates/HomeTemplate.js",["91"],"import React from 'react';\nimport { Route } from 'react-router-dom';\nimport Header from '../com