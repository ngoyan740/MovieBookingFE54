  const mangPhim = useSelector(state => state.QuanLyPhimReducer.mangPhim);\n    const dispatch = useDispatch();\n    console.log('MangPhim', mangPhim);\n    const loadDataPhim = async () => {\n        dispatch(getDataFilmAction())\n    }\n    useEffect(() => {\n\n        //Chạy 1 lần duy nhất sau khi giao diện render\n        dispatch(getDataFilmAction())\n        //Vừa vào trang sẽ load danh sách phim\n\n    }, [])\n    \n    // console.log('Props Mang Phim', props.mangPhim);\n\n    // const loadDataPhim = async() => {\n\n    //     // props.dispatch({\n    //     //     type: 'GET_DATA_FILM',\n    //     //     dataFilm: result.data\n    //     // })\n\n    //     props.dispatch(getDataFilmAction())\n    //     // Maintain code dễ hơn, chỉ handle action chi tiết được gọi trong folder actions\n    // }\n\n\n    //useEffect thay thế cho các lifecycle (didMount, didUpdate, didUnmount)\n    \n\n    const renderPhim = () => {\n        return mangPhim.map((phim, index) => {\n            return <div className=\"col-4 mb-2\">\n                <div className=\"card text-left\">\n                    <img className=\"card-img-top\" src={phim.hinhAnh} alt />\n                    <div className=\"card-body\">\n                        <h4 className=\"card-title\">{phim.tenPhim}</h4>\n                        <p className=\"card-text\">{phim.moTa}</p>\n                        <NavLink className=\"btn btn-warning\" to={`/chitietphim/${phim.maPhim}`}>Mua vé</NavLink>\n                    </div>\n                </div>\n\n            </div>\n        })\n    }\n\n    return (\n        <div className=\"container\">\n            <button onClick={() => {\n                loadDataPhim();\n            }}>Load Danh Sách Phim</button>\n            <div className=\"row mt-2\">\n                {renderPhim()}\n            </div>\n        </div>\n    )\n}\n\n\n// //Kết nối với Reducer để lấy dữ liệu mảng phim về => tạo ra được props.mangPhim\n// const mapStateToProps = (state) => {\n//     //trả về 1 state\n\n//     return {\n//         mangPhim: state.QuanLyPhimReducer.mangPhim\n//     }\n// }\n\n// export default connect(mapStateToProps) (TrangChu)","/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/pages/DangNhap/DangNhap.js",[],"/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/pages/DangKy/DangKy.js",[],"/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/pages/ChiTietPhim/ChiTietPhim.js",["70","71"],"/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/components/Header/Header.js",["72","73","74","75","76"],"import React from 'react'\nimport { NavLink } from 'react-router-dom'\nimport './header.css'\n\n\n//NavLink : là thay thế thẻ a trong react-router-dom -> chuyển trang mà không load lại trang chủ\n// <NavLink className=\"nav-link\" to=\"/trangchu\">\n\nexport default function Header() {\n    return (\n        <div className=\"border-bottom-0 border-light\">\n            <nav className=\"navbar navbar-expand-sm navbar-dark\" style={{ backgroundColor: '#343a3f' }}>\n                <a className=\"navbar-brand text-white mr-4\" href=\"/trangchu\"><img src=\"../img/movie.png\" width=\"50\" height=\"50\"/></a>\n                <button className=\"navbar-toggler d-lg-none\" type=\"button\" data-toggle=\"collapse\" data-target=\"#collapsibleNavId\" aria-controls=\"collapsibleNavId\" aria-expanded=\"false\" aria-label=\"Toggle navigation\" />\n                <div className=\"collapse navbar-collapse\" id=\"collapsibleNavId\">\n                    <ul className=\"navbar-nav mr-auto mt-2 mt-lg-0\">\n                        <li className=\"nav-item\">\n                            <NavLink activeStyle={{color: '#ffc007'}} className=\"nav-link\" to=\"/trangchu\">Trang Chủ</NavLink>\n                        </li>\n                        <li className=\"nav-item\">\n                            <NavLink activeStyle={{color: '#ffc007'}} className=\"nav-link\" to=\"/dangky\">Đăng Ký</NavLink>\n                        </li>\n                        <li className=\"nav-item\">\n                            <NavLink activeStyle={{color: '#ffc007'}} className=\"nav-link\" to=\"/dangnhap\">Đăng Nhập</NavLink>\n                        </li>\n                        <li className=\"nav-item dropdown\">\n                            <a className=\"nav-link dropdown-toggle\" href=\"#\" id=\"dropdownId\" data-toggle=\"dropdown\" aria-haspopup=\"true\" aria-expanded=\"false\">Dropdown</a>\n                            <div className=\"dropdown-menu\" aria-labelledby=\"dropdownId\">\n                                <a activeClassName=\"bg-white rounded text-dark\" className=\"dropdown-item\" to=\"#\">Action 1</a>\n                                <a activeClassName=\"bg-white rounded text-dark\" className=\"dropdown-item\" to=\"#\">Action 2</a>\n                            </div>\n                        </li>\n                    </ul>\n                    <form className=\"form-inline my-2 my-lg-0\">\n                        <input className=\"form-control mr-sm-2\" type=\"text\" placeholder=\"Search\" />\n                        <button className=\"btn btn-warning my-2 my-sm-0\" type=\"submit\">Search</button>\n                    </form>\n                </div>\n            </nav>\n        </div>\n\n    )\n}\n","/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/redux/reducers/QuanLyPhimReducer.js",["77"],"// mảng phim sẽ chứa dữ liệu phim từ api trả về\n\nimport { GET_DATA_FILM } from \"../const/QuanLyPhimConst\"\n\nconst stateDefault = {\n    mangPhim : []\n\n}\n\nexport const QuanLyPhimReducer = (state = stateDefault, action) => {\n\n    switch(action.type) {\n\n        case GET_DATA_FILM : {\n           \n            return {...state, mangPhim : action.dataFilm}\n        }\n            \n\n    }\n\n\n    return {...state}\n}","/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54/src/redux/actions/QuanLyPhimActions.js",[],"/Volumes/Home/Zi-Learn/BookingMovieFE54/bookingmoviefe54